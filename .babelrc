{
  "presets": [[
    "@babel/env",
    {
      "targets": {
        "browsers": [
          "last 2 versions",
          "safari >= 7"
        ]
      }
    }
  ]],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "AVA": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "build/webpack.config.prod.js"
          }
        ]
      ]
    }
  }
}
